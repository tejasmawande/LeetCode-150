class Solution {
    public boolean isPalindrome(int x) {
        int ans = 0;
        int a = x;
        while(x>0){
            int n = x% 10;
             ans = ans * 10 + n;
             x = x/10;
             
        } 
        if(ans == a){
            return true;
        }  
        return false;
    }
}
